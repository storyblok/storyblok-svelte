{
  "name": "@storyblok/svelte",
  "version": "1.0.0",
  "type": "module",
  "description": "Storyblok SDK to connect Storyblok with Svelte",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "svelte": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "svelte-package -i pkg --watch",
    "build": "svelte-package -i pkg",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest __tests__",
    "prepublishOnly": "npm run build && cp ../README.md ."
  },
  "dependencies": {
    "@storyblok/js": "^3.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@sveltejs/package": "^2.3.4",
    "@tsconfig/svelte": "^5.0.4",
    "babel-jest": "^29",
    "eslint-plugin-jest": "^28.8.0",
    "jest": "^29.7.0",
    "svelte": "^4.2.16",
    "svelte-preprocess": "^5.1.4",
    "typescript": "^5.5.4",
    "vite": "^6.0.0"
  },
  "peerDependencies": {
    "svelte": "^3.0.0 || ^4.0.0"
  },
  "eslintConfig": {
    "env": {
      "node": true
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/storyblok/storyblok-svelte"
  },
  "keywords": [
    "svelte",
    "storyblok"
  ],
  "author": "Josefine Schaefer",
  "bugs": {
    "url": "https://github.com/storyblok/storyblok-svelte/issues"
  },
  "homepage": "https://github.com/storyblok/storyblok-svelte",
  "release": {
    "branches": [
      {
        "name": "v2",
        "range": "2.x",
        "channel": "v2"
      },
      {
        "name": "main"
      }
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
